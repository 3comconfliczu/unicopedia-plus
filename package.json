{
    "name": "unicode-plus",
    "productName": "Unicode Plus",
    "version": "1.18.1",
    "description": "Set of Unicode, Unihan & emoji utilities.",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build-darwin-no-asar": "electron-packager . --out=build --platform=darwin --ignore=/~.* --ignore=^/screenshots --icon=icons/icon.icns --overwrite",
        "build-darwin": "electron-packager . --out=build --platform=darwin --ignore=/~.* --ignore=^/screenshots --asar.unpack=LICENSE.txt --icon=icons/icon.icns --overwrite",
        "build-linux": "electron-packager . --out=build --platform=linux --ignore=/~.* --ignore=^/screenshots --asar.unpack=LICENSE.txt --icon=icons/icon.png --overwrite",
        "build-mas": "electron-packager . --out=build --platform=mas --ignore=/~.* --ignore=^/screenshots --asar.unpack=LICENSE.txt --icon=icons/icon.icns --overwrite",
        "build-win32": "electron-packager . --out=build --platform=win32 --ignore=/~.* --ignore=^/screenshots --asar.unpack=LICENSE.txt --icon=icons/icon.ico --overwrite",
        "build-all": "electron-packager . --out=build --platform=all --ignore=/~.* --ignore=^/screenshots --asar.unpack=LICENSE.txt --icon=icons/icon --overwrite"
    },
    "keywords": [
        "app",
        "cjk",
        "desktop",
        "electron",
        "emoji",
        "javascript",
        "modular",
        "regex",
        "unicode",
        "unihan",
        "utility"
    ],
    "author": {
        "name": "Michel MARIANI",
        "url": "https://github.com/tonton-pixel"
    },
    "homepage": "https://github.com/tonton-pixel/unicode-plus",
    "license": "MIT",
    "private": false,
    "dependencies": {
        "emoji-patterns": "^11.2.2",
        "emoji-test-groups": "^11.0.0",
        "emoji-test-list": "^11.0.0",
        "emoji-test-patterns": "^11.0.1",
        "regexpu-core": "^4.4.0"
    },
    "devDependencies": {
        "electron": "^3.0.13",
        "electron-packager": "^13.0.1"
    }
}
